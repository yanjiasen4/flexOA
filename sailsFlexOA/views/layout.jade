  body.page-header-fixed.page-sidebar-closed-hide-logo.page-content-white
    block header
      .page-header.navbar.navbar-fixed-top
        .page-header-inner
          .page-logo
            a(href='/') 
              img.logo-default(alt='logo', src='/layouts/layout/img/logo.png')
            .menu-toggler.sidebar-toggler
          .menu-toggler.responsive-toggler(href='javascript:;', data-toggle='collapse', data-target='.navbar-collapse')
          .top-menu
            ul.nav.navbar-nav.pull-right
              li.dropdown.dropdown-user
                a.dropdown-toggle(href='javascript:;', data-toggle='dropdown', data-close-others='true', data-hover='dropdown')
                  span.username.username-hide-on-mobile username
      .clearfix
    .page-container
      .page-sidebar-wrapper
        block sidebar
          .page-sidebar.navbar-collapse.collapse
            ul.page-sidebar-menu.page-header-fixed(style='padding-top: 20px', data-slide-speed='200', data-auto-scroll='true', data-keep-expanded='false')
              li.sidebar-toggler-wrapper.hide
                .sidebar-toggler
              li.nav-item.start.active.open
                a.nav-link.nav-toggle(href='javascript:;')
                  i.icon-home
                  span.title 控制台
                  span.selected
                  span.arrow.open
                ul.sub-menu(style='display:block')
                  li.nav-item.start.active.open
                    a.nav-link(href='/')
                      i.icon-bar-chart
                      span.title whatever1
                      span.selected
                  li.nav-item.start.active.open
                    a.nav-link(href='/')
                      i.icon-bulb
                      span.title whatever2
                  li.nav-item.start.active.open
                    a.nav-link(href='/')
                      i.icon-graph
                      span.title whatever3                   
      //  +list(0)
      .page-content-wrapper
        .page-content(style='min-height:1135px')
          block content-header
            .theme-panel.hidden-xs.hidden-sm
            .page-bar
              ul.page-breadcrumb
                li
                  a(href='#') 首页
                  i.fa.fa-circle
                li
                  span 页面1
              .page-toolbar
                .pull-right.tooltips.btn.btn-sm(id='dashboard-report-range', data-original-title='更改设定日期', data-container='body', data-placement='bottom')
                  i.icon-calendar
                  span.thin.uppercase.hidden-xs= date
                  i.fa.fa-angle-down
           h3.page-title= title
             small= titleIntro
           block content
    .page-footer
      block footer
